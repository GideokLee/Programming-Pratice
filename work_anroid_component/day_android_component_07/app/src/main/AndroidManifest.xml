<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ssafy.comp_07.app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.SSAFY1">
        <activity android:name=".TextureViewActivity" />
        <activity android:name=".SearchViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".RatingBarActivity" />
        <activity android:name=".SeekBarActivity" />
        <activity android:name=".progressbar.ProgressBarActivity2" />
        <activity android:name=".progressbar.ProgressBarActivity" />
        <activity android:name=".CalendarViewActivity" />
        <activity android:name=".VideoViewActivity" />
        <activity android:name=".WebViewActivity"/>
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>